[Unit]
Description=Restore ipset sets at boot
Before=iptables.service

[Service]
Type=oneshot
Environment=/usr/lib/safe-rm:/usr/bin/vendor_perl:%h/go/bin:%h/.cargo/bin:%h/.local/bin:%h/dotfiles-linux/bin:/usr/local/sbin:/usr/local/bin:/usr/bin:/usr/lib/jvm/default/bin:/usr/bin/site_perl:/usr/bin/core_perl
ExecStart=%h/dotfiles-linux/bin/ipset-load blocked

[Install]
WantedBy=default.target

